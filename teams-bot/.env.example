# Required
FOUNDRY_AGENT_URL=https://your-foundry-agent-endpoint
FOUNDRY_AGENT_TOKEN=your_bearer_token

# Optional
PORT=3978
# Provide additional headers as JSON (e.g., org/workspace identifiers)
FOUNDRY_AGENT_EXTRA_HEADERS={}
# Payload key to send text under (default: text). For some agents use "input" or "message".
FOUNDRY_AGENT_PAYLOAD_KEY=text
